@model IEnumerable<ArtistAgencyWeb.DTOs.MusicianDTO>
@{
    Layout = null;
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                <a href="#" onclick="LoadEditView('@item.Id')">Edit</a> |
                <a href="#" onclick="LoadDetailsView('@item.Id')">Details</a> |
                <a href="#" onclick="LoadDeleteView('@item.Id')">Delete</a>
            </td>
        </tr>
    }

</table>